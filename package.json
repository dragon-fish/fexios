{
  "name": "fexios",
  "version": "6.2.0",
  "description": "Fetch based HTTP client with similar API to axios for browser and Node.js",
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "lib"
  ],
  "sideEffects": false,
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./core": {
      "import": "./dist/fexios.mjs",
      "require": "./dist/fexios.cjs"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./models": {
      "import": "./dist/models/index.mjs",
      "require": "./dist/models/index.cjs"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.cjs"
    },
    "./plugins": {
      "import": "./dist/plugins/index.mjs",
      "require": "./dist/plugins/index.cjs"
    }
  },
  "scripts": {
    "build": "run-p build:*",
    "build:import": "unbuild",
    "build:bundle": "cross-env vite build",
    "clean": "rimraf dist lib",
    "test": "vitest run ./test ./src",
    "prepublish": "npm run build",
    "review": "vite preview --outDir ./.test_reports"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "fetch",
    "ajax",
    "http",
    "promise",
    "hookable",
    "node",
    "browser",
    "axios"
  ],
  "author": "dragon-fish <dragon-fish@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dragon-fish/fexios.git"
  },
  "bugs": {
    "url": "https://github.com/dragon-fish/fexios/issues"
  },
  "homepage": "https://github.com/dragon-fish/fexios#readme",
  "engines": {
    "node": "^16.15.0 || >=18.0.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.55.2",
    "@types/node": "^24.10.9",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "@web-std/file": "^3.0.3",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eventsource": "^4.1.0",
    "fexios": "workspace:",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "unplugin-dts": "1.0.0-beta.6",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}